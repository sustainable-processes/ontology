<rdf:RDF xmlns="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/molecular_structure.owl#"
     xml:base="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/molecular_structure.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
