     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:system="https://github.com/sustainable-processes/ontology/OntoCAPE/upper_level/system.owl#"
     xmlns:der_dim="https://github.com/sustainable-processes/ontology/OntoCAPE/supporting_concepts/physical_dimension/derived_dimensions.owl"
     xmlns:multiset="https://github.com/sustainable-processes/ontology/meta_model/data_structures/multiset.owl#"
     xmlns:substance="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#"
     xmlns:molecular_structure="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/molecular_structure.owl"
     xmlns:fundamental_concepts="https://github.com/sustainable-processes/ontology/meta_model/fundamental_concepts/fundamental_concepts.owl#"
     xmlns:mathematical_relation="https://github.com/sustainable-processes/ontology/OntoCAPE/supporting_concepts/mathematical_relation/mathematical_relation.owl#">
    <owl:Ontology rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/molecular_structure.owl">
        <owl:imports rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl"/>
