        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ReversibleReaction -->

    <owl:Class rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ReversibleReaction">
