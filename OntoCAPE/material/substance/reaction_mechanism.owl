    <!-- https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ChemicalReaction -->

    <owl:Class rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#ChemicalReaction">
        <rdfs:subClassOf rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/upper_level/system.owl#System"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasProduct"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#ChemicalSpecies"/>
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasReactant"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#ChemicalSpecies"/>
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasProduct"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#ChemicalSpecies"/>
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasReactant"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#ChemicalSpecies"/>
                            <rdf:Description rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance.owl#MolecularEntity"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasStoichiometricCoefficient"/>
                <owl:allValuesFrom rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#StoichiometricCoefficient"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasProduct"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasReactant"/>
                <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#hasStoichiometricCoefficient"/>
