        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>The attribute stoichiometricValue specifies the numerical value of a StoichiometricCoefficient. It is positive for products and negative for reactants.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://github.com/sustainable-processes/ontology/OntoCAPE/meta_model/fundamental_concepts/fundamental_concepts.owl#relationAttribute -->

