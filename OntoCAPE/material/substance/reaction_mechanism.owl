    <!-- https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#stoichiometricValue -->

    <owl:DatatypeProperty rdf:about="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/reaction_mechanism.owl#stoichiometricValue">
        <rdfs:subPropertyOf rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/meta_model/fundamental_concepts/fundamental_concepts.owl#relationAttribute"/>
