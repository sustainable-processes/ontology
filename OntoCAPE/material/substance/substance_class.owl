     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:root="https://github.com/sustainable-processes/ontology/OntoCAPE/"
     xmlns:array="https://github.com/sustainable-processes/ontology/meta_model/data_structures/array.owl#"
