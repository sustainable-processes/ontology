                                <owl:hasValue rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/material/substance/substance_class.owl#KetoneGroup"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="https://github.com/sustainable-processes/ontology/OntoCAPE/upper_level/system.owl#contains"/>
