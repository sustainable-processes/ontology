  <!ENTITY apparatus    "&root;OntoCAPE/chemical_process_system/CPS_realization/plant_equipment/apparatus.owl">
  <!ENTITY fixture  "&root;OntoCAPE/chemical_process_system/CPS_realization/plant_equipment/fixture.owl">
  <!ENTITY distillation_system  "&root;OntoCAPE/chemical_process_system/process_units/distillation_system.owl">
  <!ENTITY heat_transfer_unit   "&root;OntoCAPE/chemical_process_system/process_units/heat_transfer_unit.owl">
  <!ENTITY splitting_unit   "&root;OntoCAPE/chemical_process_system/process_units/splitting_unit.owl">
]>

<rdf:RDF
    xmlns:behavior="&behavior;#"
    xmlns:heat_transfer_unit="&heat_transfer_unit;#"
    xmlns:material="&material;#"
    xmlns:rdfs="&rdfs;#"
    xmlns:owl="&owl;#"
    xmlns:terms="http://purl.org/dc/terms/"
    xmlns:chemical_process_system="&chemical_process_system;#"
    xmlns:phase_system="&phase_system;#"
    xmlns:process="&process;#"
    xmlns:plant="&plant;#"
    xmlns:system="&system;#"
    xmlns:process_ext="&process_ext;#"
    xmlns:rdf="&rdf;#"
    xmlns:xsd="&xsd;#"
    xmlns:splitting_unit="&splitting_unit;#"
    xmlns="&distillation_system;#"
  xml:base="&distillation_system;">
  <owl:Ontology rdf:about="&distillation_system;">
    <rdfs:comment rdf:datatype="&xsd;#string"
    >The ontology module 'distillation_system' provides concepts for the detailed description of a distillation system applicable in a chemical process system.
The following classes and relations from other ontology modules are used within 'distillation_system': 
    
&lt;owl:Class rdf:about="&behavior;#FilmConnection"/&gt; 
&lt;owl:Class rdf:about="&behavior;#MaterialStream"/&gt; 
&lt;owl:Class rdf:about="&material;#Material"/&gt; 
&lt;owl:Class rdf:about="&chemical_process_system;#ProcessUnit"/&gt; 
&lt;owl:Class rdf:about="&heat_transfer_unit;#HeatTransferUnit"/&gt; 
&lt;owl:Class rdf:about="&splitting_unit;#SplittingUnit"/&gt; 
&lt;owl:Class rdf:about="&phase_system;#SinglePhase"/&gt; 
&lt;owl:Class rdf:about="&apparatus;#TrayColumn"/&gt; 
&lt;owl:Class rdf:about="&process_ext;#Distillation"/&gt; 
&lt;owl:Class rdf:about="&phase_system;#Pressure"/&gt; 
&lt;owl:Class rdf:about="&system;#ScalarQuantity"/&gt;
&lt;owl:Class rdf:about="&behavior;#MaterialAmount"/&gt;
&lt;owl:Class rdf:about="&fixture;#Tray"/&gt;
&lt;owl:Class rdf:about="&system;#ExtensibleValueSet"/&gt;
&lt;owl:Class rdf:about="&heat_transfer_unit;#HeatTransferUnitBehavior"/&gt;
&lt;owl:Class rdf:about="&splitting_unit;#SplittingBehavior"/&gt;
&lt;owl:Class rdf:about="&process;#ProcessState"/&gt;
&lt;owl:Class rdf:about="&phase_system;#MultiphaseSystem"/&gt;
&lt;owl:Class rdf:about="&phase_system;#SinglePhaseInMultiphaseSystem"/&gt;
&lt;owl:Class rdf:about="&behavior;#MaterialAmountConnection"/&gt;
&lt;owl:Class rdf:about="&phase_system;#Pressure"/&gt;
&lt;owl:Class rdf:about="&process;#ProcessStepProperty"/&gt;
                                  
&lt;owl:ObjectProperty rdf:about="&system;#hasDirectSubsystem"/&gt; 
&lt;owl:ObjectProperty rdf:about="&behavior;#refersToMaterial"/&gt; 
&lt;owl:ObjectProperty rdf:about="&technical_system;#hasBehavioralAspect"/&gt; 
&lt;owl:ObjectProperty rdf:about="&technical_system;#hasFunctionalAspect"/&gt; 
&lt;owl:ObjectProperty rdf:about="&technical_system;#hasRealizationAspect"/&gt; 
&lt;owl:ObjectProperty rdf:about="&system;#hasProperty"/&gt;
&lt;owl:ObjectProperty rdf:about="&technical_system;#representsBehaviorOf"/&gt;
&lt;owl:ObjectProperty rdf:about="&plant;#hasFixture"/&gt;
&lt;owl:ObjectProperty rdf:about="&process;#hasBottomProduct"/&gt;
&lt;owl:ObjectProperty rdf:about="&chemical_process_system;#refersToGeneralizedAmount"/&gt;
&lt;owl:ObjectProperty rdf:about="&process;#hasInputAtNumeredTray"/&gt;
&lt;owl:ObjectProperty rdf:about="&process;#hasTopProduct"/&gt;
&lt;owl:ObjectProperty rdf:about="&technical_system;#hasPhenomenon"/&gt;
&lt;owl:ObjectProperty rdf:about="&material;#thermodynamicBehavior"/&gt;
&lt;owl:ObjectProperty rdf:about="&system;#isComposedOfSubsystem"/&gt;
&lt;owl:ObjectProperty rdf:about="&phase_system;#hasStateOfAggregation"/&gt;
&lt;owl:ObjectProperty rdf:about="&network_system;#leaves"/&gt;
&lt;owl:ObjectProperty rdf:about="&network_system;#enters"/&gt;
&lt;owl:ObjectProperty rdf:about="&network_system;#hasOutput"/&gt;
&lt;owl:ObjectProperty rdf:about="&system;#hasCharacteristic"/&gt;
&lt;owl:ObjectProperty rdf:about="&network_system;#hasInput"/&gt;
    
&lt;system:QualitativeValue rdf:about="&phase_system;#gaseous"/&gt; 
&lt;system:QualitativeValue rdf:about="&phase_system;#liquid"/&gt;
&lt;behavior:MolecularTransportPhenomenon rdf:about="&behavior;#mass_diffusion"/&gt;
