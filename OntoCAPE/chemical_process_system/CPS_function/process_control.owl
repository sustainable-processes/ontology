        <owl:differentFrom rdf:resource="https://raw.githubusercontent.com/sustainable-processes/ontology/refs/heads/main/OntoCAPE/chemical_process_system/CPS_function/process_control.owl#nonlinear"/>
        <rdfs:comment>Linear refers to the behavior of the ControlledSystem.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/ontology/refs/heads/main/OntoCAPE/chemical_process_system/CPS_function/process_control.owl#nonlinear -->

