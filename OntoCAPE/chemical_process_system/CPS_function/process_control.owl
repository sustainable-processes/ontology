        <rdfs:comment>StateFeedbackControl is a method employed in feedback control.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- https://raw.githubusercontent.com/sustainable-processes/ontology/refs/heads/main/OntoCAPE/chemical_process_system/CPS_function/process_control.owl#linear -->

    <owl:NamedIndividual rdf:about="https://raw.githubusercontent.com/sustainable-processes/ontology/refs/heads/main/OntoCAPE/chemical_process_system/CPS_function/process_control.owl#linear">
